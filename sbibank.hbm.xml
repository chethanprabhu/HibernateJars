<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
	"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-mapping>
<class name="com.chethan.dto.AccountDTO" table="Accounts">

<id column="Account_Number" name="id" type="int">
<generator class="increment"/>
</id>
<property column="Account_Name" name="name" type="string"/>
<property column="Account_Balance" name="bal" type="int"/>

</class>
</hibernate-mapping>